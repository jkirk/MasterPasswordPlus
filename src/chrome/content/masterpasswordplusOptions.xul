<!DOCTYPE prefwindow SYSTEM "chrome://mapaplus/locale/masterpasswordplusOptions.dtd">

<prefwindow xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
						id="mapaPlusOptionsWindow"
						title="&mapaPlus.options.title;"
						ondialogaccept="mapaPlus.saveOptions()"
						ondialogcancel="mapaPlus.autoSave()"
						ondialogdisclosure="mapaPlus.about()"
						buttonlabelextra1="&mapaPlus.options.unlock;"
						ondialogextra1="mapaPlus.protectUnlock()"
						onload="mapaPlus.load()"
						persist="screenX,screenY"
						xmlns:html="http://www.w3.org/1999/xhtml"
						buttons="accept,cancel,disclosure"
>
	<script type="application/javascript" src="chrome://mapaplus/content/masterpasswordplusCommon.js"/>
	<script type="application/javascript" src="chrome://mapaplus/content/masterpasswordplusOptions.js"/>
	<script type="application/javascript">
		mapaPlus.strings = {
			moreinfo:			"&mapaPlus.options.moreinfo;",
			changesLog1:	"&mapaPlus.options.showChangesLogNotif1;",
			changesLog2:	"&mapaPlus.options.showChangesLogFull;",
			disabled:			"&mapaPlus.options.hotkey.enabled.disabled;"
		};
	</script>
	<prefpane id="masterPasswordPlusOptions" />
</prefwindow>
